WHEN @P IS A processor:
  MY PATTERN IN ( @P ) IS THE processor
  (SELECT (closingPrice) GREATER THAN (1) FROM (@P))
  AND
  (
  (X (SELECT ((closingPrice) LESS THAN (2)) AND ((stockSymbol) = (1)) FROM (@P)))
  OR
  (
  X ( (SELECT ((closingPrice) GREATER THAN (2)) AND ((stockSymbol) = (1)) FROM (@P)) AND (X (SELECT ((closingPrice) GREATER THAN (1)) AND ((stockSymbol) = (2)) FROM (@P)))
  ))).